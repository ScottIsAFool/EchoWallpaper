<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Resource dictionary entries should be defined here. -->

    <Color x:Key="PhoneAccentColor">#065EA6</Color>
    <SolidColorBrush x:Key="PhoneAccentBrush"
                     Color="{StaticResource PhoneAccentColor}"/>
    
    <Color x:Key="PhoneBackgroundColor">White</Color>
    <SolidColorBrush x:Key="PhoneBackgroundBrush"
                     Color="{StaticResource PhoneBackgroundColor}"/>
    
</ResourceDictionary>